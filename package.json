{
  "name": "ban-api",
  "version": "1.0.4-FEATURE",
  "description": "Ban API",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "new-build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "schema:sync": "typeorm schema:sync",
    "schema:drop": "typeorm schema:drop",
    "migration:generate": "typeorm migration:generate -n",
    "migration:run": "typeorm migration:run",
    "migration:revert": "typeorm migration:revert",
    "migration:create": "typeorm migration:create -n",
    "migration:show": "typeorm migration:show"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.6.0",
    "@nestjs/common": "^8.0.9",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.9",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/microservices": "^8.2.3",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.9",
    "@nestjs/platform-socket.io": "^8.0.9",
    "@nestjs/swagger": "^5.1.0",
    "@nestjs/typeorm": "^8.0.2",
    "@nestjs/websockets": "^8.0.9",
    "@types/express-handlebars": "^5.3.0",
    "amqp-connection-manager": "^3.7.0",
    "amqplib": "^0.8.0",
    "aws-sdk": "^2.922.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "express-handlebars": "^5.3.2",
    "geoip-lite": "^1.4.2",
    "handlebars": "^4.7.7",
    "hbs": "^4.1.2",
    "keycloak-connect": "^15.0.2",
    "mime": "^2.4.7",
    "moment": "^2.24.0",
    "mongodb": "^3.6.5",
    "multer-s3": "^2.9.0",
    "nest-keycloak-connect": "^1.7.6",
    "nestjs-redoc": "^2.2.2",
    "nestjs-s3": "^1.0.1",
    "nestjs-twilio": "^2.1.0",
    "nestjs-typeorm-paginate": "^2.5.1",
    "nestjs-yup": "^1.0.13",
    "nodemailer": "^6.6.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.5.1",
    "querystring": "^0.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "swagger-ui-express": "^4.1.4",
    "ts-node": "^9.1.1",
    "twilio": "^3.71.2",
    "typeorm": "^0.2.31",
    "uuid": "^8.3.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.2",
    "@nestjs/schematics": "^8.0.3",
    "@nestjs/testing": "^8.1.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/mime": "^2.0.3",
    "@types/mongodb": "^3.6.10",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.6",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^2.0.10",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
